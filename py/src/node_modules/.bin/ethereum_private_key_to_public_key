#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../ethereum-private-key-to-public-key/bin/ethereum_private_key_to_public_key" "$@"
else 
  exec node  "$basedir/../ethereum-private-key-to-public-key/bin/ethereum_private_key_to_public_key" "$@"
fi
